{
  "manifest_version": 2,

  "name": "ShaeCom",
  "description": "Turns all instances of heights into Shaeish heights",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "Click to check Shaeish's height"
  },
  "content_scripts": [
	  {
		  "matches": [
			  "*://*/*"
		  ],
		  "js": [
			  "content.js"
		  ],
		  "run_at": "document_end"
	  }
  ]
}
